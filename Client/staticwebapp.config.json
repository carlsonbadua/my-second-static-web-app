{
  "trailingSlash": "auto",
  {
    "routes": [
      {
        "route": "/",
        "rewrite": "/index.html"
      },
      {
        "route": "/profile*",
        "allowedRoles": [ "authenticated" ]
      },
      {
        "route": "/admin/index.html",
        "allowedRoles": [ "administrator" ]
      },
      {
        "route": "/images/*",
        "headers": {
          "cache-control": "must-revalidate, max-age=15770000"
        }
      },
      {
        "route": "/api/*",
        "methods": [ "GET" ],
        "allowedRoles": [ "authenticated" ]
      },
      {
        "route": "/login",
        "rewrite": "/.auth/login/github"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      },
      {
        "route": "/calendar*",
        "rewrite": "/calendar.html"
      },
      {
        "route": "/specials",
        "redirect": "/deals",
        "statusCode": 301
      },
      {
        "route": "/{*all}",
        "statusCode": 404
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [ "/images/*", "/api/*" ]
    }
  }
}